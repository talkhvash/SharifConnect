<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox fx:id="box" nodeOrientation="RIGHT_TO_LEFT" prefHeight="36.0" prefWidth="850.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.main.java.view.FXML.edu.demand.professor.minor.MinorListItemFXML">
   <children>
      <Label fx:id="nameLabel" alignment="CENTER" prefHeight="37.0" prefWidth="200.0" text="Label" />
      <Label fx:id="outLabel" alignment="CENTER" prefHeight="36.0" prefWidth="200.0" text="Label" />
      <VBox fx:id="stateBox">
         <children>
            <CheckBox fx:id="reject" mnemonicParsing="false" onAction="#reject" text="رد" />
            <CheckBox fx:id="confirm" mnemonicParsing="false" onAction="#confirm" text="تایید" />
         </children>
      </VBox>
      <Label fx:id="inLabel" alignment="CENTER" prefHeight="36.0" prefWidth="200.0" text="Label" />
      <Label fx:id="departmentStateLabel" alignment="CENTER" prefHeight="37.0" prefWidth="100.0" text="Label" />
      <Label fx:id="stateLabel" alignment="CENTER" prefHeight="36.0" prefWidth="100.0" text="Label">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </Label>
   </children>
</HBox>
